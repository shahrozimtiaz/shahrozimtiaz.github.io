package project1;

import java.util.Scanner;

public class Bottle 
{
	Scanner scan = new Scanner(System.in);
	
	public void read()
	{
		scan.nextInt();
		
	}
	public int set(int a)
	{
		int value = a;
		return value;
	}
	public int add(int other)
	{
		int valueAdd = this.add(other);
		return valueAdd;
		
	}
	

}
